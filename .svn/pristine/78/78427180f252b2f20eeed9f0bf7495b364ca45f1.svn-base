package members.member.vo;

public class GameCodeVO {
	private String code_id;
	private String mem_id;
	private String game_code;
	private String game_id;
	private String code_use;

	public GameCodeVO(String code_id, String mem_id, String game_code) {
		super();
		this.code_id = code_id;
		this.mem_id = mem_id;
		this.game_code = game_code;
	}

	public GameCodeVO() {
		// TODO Auto-generated constructor stub
	}

	public String getCode_id() {
		return code_id;
	}

	public void setCode_id(String code_id) {
		this.code_id = code_id;
	}

	public String getMem_id() {
		return mem_id;
	}

	public void setMem_id(String mem_id) {
		this.mem_id = mem_id;
	}

	public String getGame_code() {
		return game_code;
	}

	public void setGame_code(String game_code) {
		this.game_code = game_code;
	}

	public String getCode_use() {
		return code_use;
	}

	public void setCode_use(String code_use) {
		this.code_use = code_use;
	}

	public String getGame_id() {
		return game_id;
	}

	public void setGame_id(String gmae_id) {
		this.game_id = gmae_id;
	}

}
